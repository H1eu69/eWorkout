<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Button1">
        <item name="android:layout_width">288dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/montserrat</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Button2">
        <item name="android:layout_width">288dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:drawableLeft">@drawable/google_logo</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/montserrat</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Button3">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:drawable">@drawable/notification_icon</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginTop">35dp</item>
    </style>

    <style name="Button4">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Button5">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#92A3FD</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="EditText1" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">288dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxStrokeColor">@color/edit_text_1_stroke_color</item>
        <item name="boxStrokeWidth">1dp</item>
    </style>

    <style name="ThemeEditText1" parent="">
        <item name="colorPrimary">@color/edit_text_1_stroke_color</item>
    </style>

</resources>